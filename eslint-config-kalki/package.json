{
  "name": "@kalki/eslint-config-kalki",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./flat/library": "./library.eslint.config.js",
    ".": "./library.eslint.config.js"
  },
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
    "@types/eslint": "^8.56.7",
    "eslint": "^8.57.0",
    "globals": "^15.0.0"
  },
  "scripts": {
    "g:lint": "cd $INIT_CWD && eslint .",
    "g:lint-fix": "cd $INIT_CWD && eslint . --fix",
    "g:lint-to-log": "cd $INIT_CWD && eslint . -o eslintReport.html"
  },
  "nx": {
    "includedScripts": [
      "g:lint",
      "g:lint-fix",
      "g:lint-to-log"
    ]
  }
}
